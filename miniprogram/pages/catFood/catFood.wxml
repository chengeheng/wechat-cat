<!-- miniprogram/pages/catFood/catFood.wxml -->
<!-- 喵粮详情 -->
<view class="form">
    <view class="form-item">
        <view class="form-item-title-request">喵粮品种</view>
        <view class="form-item-body">
            <input class="form-item-body-text" auto-focus bindblur="bindNameBlur" placeholder="请输入喵粮品种"></input>
        </view>
    </view>
    <view class="form-item">
        <view class="form-item-title-request">花费</view>
        <view class="form-item-body">
            <input class="form-item-body-text" type="number" bindblur="bindCostBlur" placeholder="请输入花费"></input>
        </view>
    </view>
    <!-- <view class="form-image">
        <view class="form-image-uploader">
            <mp-cells>
